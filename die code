#quadratische Gleichungen
import math
a=input('Geben sie a ein: ')
b=input('Geben sie b ein: ')
c=input('Geben sie c ein: ')
a=int(a)
b=int(b)
c=int(c)
D=b**2-4*a*c
wurzD=math.sqrt(D)
if(D>0):
    x1=(-b+wurzD)/2
    x2=(-b-wurzD)/2
    print('zwei Lösungen', x1, x2)
elif(D==0):
    x=-b/2
    print('eine doppelte Lösungen', x)
else:
    print('keine Lösung')
